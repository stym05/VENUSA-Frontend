{
  "expo": {
    "name": "VENUSA",
    "slug": "VENUSA",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_round.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo_round.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_round.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/logo_round.png",
      "output": "static"
    },
    "platforms": ["ios", "android", "web"],
    "plugins": [
      "expo-font",
      "@react-native-async-storage/async-storage"
    ]
  }
}